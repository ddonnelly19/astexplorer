{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ESNext",
		"jsx": "react",
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"outDir": "out"
	},
	"exclude": [
		"node_modules",
		"**/node_modules/*"
	]
}